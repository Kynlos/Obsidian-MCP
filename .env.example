# Obsidian MCP Configuration
# Copy this file to .env and configure for your system

# Path to your Obsidian vault
# Windows example: C:/Users/YourName/Documents/MyVault
# macOS/Linux example: /Users/YourName/Documents/MyVault
OBSIDIAN_VAULT_PATH=/path/to/your/vault

# Optional: Default vault name (if you have multiple vaults)
OBSIDIAN_DEFAULT_VAULT=MyVault

# Optional: Enable debug logging
OBSIDIAN_DEBUG=false
